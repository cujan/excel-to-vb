      <Query>
        <DataSourceName>spzConnectionString</DataSourceName>
        <CommandText>SELECT     all_clenovia.meno, all_clenovia.priezvisko AS clen_priezvisko, all_clenovia.rodne_cislo, all_clenovia.mesto, all_clenovia.okres_bydliska, 
                      zdruzenia.nazov AS zdruzenie_nazov, 
                      CASE WHEN all_clenovia.brokova_zbran LIKE 'ÁNO' THEN 'Broková zbran' ELSE all_clenovia.brokova_zbran END AS typ_zbran, 
                      all_clenovia.kontrolne_strelby_brok AS zbran_strelby_kedy, titul_pred.skratka, titul_za.skratka AS Expr1
FROM         all_clenovia INNER JOIN
                      zdruzenia ON all_clenovia.clen_pz = zdruzenia.ico LEFT OUTER JOIN
                      titul_pred ON titul_pred.id = all_clenovia.titul_pred_id LEFT OUTER JOIN
                      titul_za ON titul_za.id = all_clenovia.titul_za_id
WHERE     (all_clenovia.brokova_zbran LIKE @Param1) AND (DATEPART(YEAR, GETDATE()) - all_clenovia.kontrolne_strelby_brok &gt;= 5) AND 
                      (zdruzenia.nazov LIKE @Param2)
UNION ALL
SELECT     all_clenovia_1.meno, all_clenovia_1.priezvisko AS clen_priezvisko, all_clenovia_1.rodne_cislo, all_clenovia_1.mesto, all_clenovia_1.okres_bydliska, 
                      zdruzenia_1.nazov AS zdruzenie_nazov, 
                      CASE WHEN all_clenovia_1.gulova_zbran LIKE 'ÁNO' THEN 'Gulovná zbran' ELSE all_clenovia_1.gulova_zbran END AS typ_zbran, 
                      all_clenovia_1.kontrolne_strelby_gula AS zbran_strelby_kedy, titul_pred_1.skratka, titul_za_1.skratka AS Expr1
FROM         all_clenovia AS all_clenovia_1 INNER JOIN
                      zdruzenia AS zdruzenia_1 ON all_clenovia_1.clen_pz = zdruzenia_1.ico LEFT OUTER JOIN
                      titul_pred AS titul_pred_1 ON titul_pred_1.id = all_clenovia_1.titul_pred_id LEFT OUTER JOIN
                      titul_za AS titul_za_1 ON titul_za_1.id = all_clenovia_1.titul_za_id
WHERE     (all_clenovia_1.gulova_zbran LIKE @Param3) AND (DATEPART(YEAR, GETDATE()) - all_clenovia_1.kontrolne_strelby_gula &gt;= 5) AND 
                      (zdruzenia_1.nazov LIKE @Param4)
ORDER BY clen_priezvisko</CommandText>
