      <Query>
        <DataSourceName>spzConnectionString</DataSourceName>
        <CommandText>SELECT     all_clenovia.meno, all_clenovia.priezvisko, all_clenovia.datum_narodenia, all_clenovia.okres_bydliska, all_clenovia.psc, all_clenovia.telefon, 
                      all_clenovia.mesto, all_clenovia.ulica, all_clenovia.ulica_cislo, zdruzenia.nazov
FROM         all_clenovia LEFT OUTER JOIN
                      zdruzenia ON zdruzenia.ico = all_clenovia.clen_pz
WHERE     (DATEPART(mm, all_clenovia.datum_narodenia) LIKE @Param2) AND (zdruzenia.nazov LIKE @Param3)</CommandText>
